@section scriptF{
    <script>
        var auxbol = 1;
        $(document).ready(function () {
            $("#ListGeneros").val('@Model.id_genero');
            $("#ListOcupaciones").val('@Model.id_ocupacion');
            $("#ListEstado").val('@Model.id_estado');
            getMunicipio('@Model.id_estado');

            $("#ListEstado").change(function () {
                var id = $("#ListEstado").val();
                getMunicipio(id);
            });
            function getMunicipio(id) {
                $.ajax({
                    url: "/Micuenta/Municipio",
                    data: { id: id },
                    dataType: "json",
                    type: "POST",
                    error: function () {
                        $("#ListMunicipio").html('');
                    },
                    success: function (municipio) {
                        var items = "";
                        for (var i = 0; i < municipio.length; i++) {
                            items += "<option value=\"" + municipio[i].id_municipio + "\">" + municipio[i].descripcion + "</option>";
                        }
                        $("#ListMunicipio").html(items);
                        if (auxbol == 1) {
                            $("#ListMunicipio").val('@Model.id_municipio');
                            auxbol = 0;
                        }
                    }
                });
            }
        });
</script>
